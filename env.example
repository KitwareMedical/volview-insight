# VolView Insight Environment Configuration
# Copy this file to .env and fill in your values

# Hugging Face API Token (required for medGemma)
HF_TOKEN=

# Service URLs
ORTHANC_URL=http://orthanc:8042
HAPI_FHIR_URL=http://hapi-fhir:3000/hapi-fhir-jpaserver/fhir/
BACKEND_URL=http://backend:4014/

# Development settings
NODE_ENV=development
PYTHON_ENV=development

# Model settings
MEDGEMMA_MODEL_VARIANT=4b-it
MONAI_MODEL_ID=microsoft/BiomedNLP-BiomedBERT-base-uncased-abstract-fulltext

# Storage paths
ORTHANC_DATA_PATH=./volumes/orthanc-data
HAPI_FHIR_DATA_PATH=./volumes/hapi-fhir-data
MODEL_CACHE_PATH=./volumes/model-cache

# Ports
FRONTEND_PORT=8080
BACKEND_PORT=4014
ORTHANC_PORT=8042
HAPI_FHIR_PORT=3000
ORTHANC_PROXY_PORT=5173
